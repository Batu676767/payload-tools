{
  "name": "@payload-tools/author-fields",
  "version": "1.2.2",
  "private": false,
  "bugs": "https://github.com/shefing/payload-tools/issues",
  "repository": "https://github.com/shefing/payload-tools",
  "license": "MIT",
  "author": "shefing",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/authorFieldPlugin.ts",
      "require": "./src/authorFieldPlugin.ts",
      "types": "./src/authorFieldPlugin.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "devDependencies": {
    "@payloadcms/ui": "3.0.0-beta.122",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "payload": "3.0.0-beta.122",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-dom": "19.0.0-rc-65a56d0e-20241020",
    "typescript": "^5.5.2",
    "moment": "^2.30.1"
  },
  "peerDependencies": {
    "@payloadcms/ui": "^3.0.0-beta.122",
    "payload": "3.0.0-beta.122"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/authorFieldPlugin.js",
        "require": "./dist/authorFieldPlugin.js",
        "types": "./dist/authorFieldPlugin.d.ts"
      }
    },
    "main": "./dist/authorFieldPlugin.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/authorFieldPlugin.d.ts"
  }
}
